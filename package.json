{
	"name": "@mbss/nestjs-core",
	"version": "1.0.8",
	"description": "MBSS-Library core package for nestjs, includes default functions to boost startup with nestjs backend from scratch",
	"title": "MBSS-Nestjs Core package",
	"keywords": [
		"nestjs",
		"core",
		"backend",
		"functions",
		"scratch",
		"startup"
	],
	"author": "Michael Bigalke & Sven Stiels",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mbss-libraries/nestjs-core"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"private": false,
	"scripts": {
		"---------- PUBLISH -------------------------------------------------------------------------------------": "",
		"npm:publish": "npm publish --access public",
		"prepare": "npm run build",
		"prepublishOnly:old": "npm run test & npm run lint",
		"prepublishOnly": "npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"---------- TEST -------------------------------------------------------------------------------------": "",
		"test": "jest --config jestconfig.json",
		"build": "tsc && tsc-alias",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"lint:fix": "tslint --fix -p tsconfig.json -c tslint.json"
	},
	"files": [
		"dist",
		"lib",
		"es"
	],
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"Firefox ESR",
		"not dead",
		"IE 11",
		"not IE 10"
	],
	"dependencies": {
		"@nestjs/core": "^9.0.11",
		"@nestjs/jwt": "^9.0.0",
		"@nestjs/passport": "^9.0.0",
		"@nestjs/platform-express": "^9.0.11",
		"@nestjs/schedule": "^2.1.0",
		"@nestjs/serve-static": "^3.0.0",
		"@nestjs/typeorm": "^9.0.1",
		"bcrypt": "^5.0.1",
		"cache-manager": "^4.1.0",
		"chalk": "^5.0.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"compression": "^1.7.4",
		"dotenv": "^16.0.2",
		"express-rate-limit": "^6.6.0",
		"google-auth-library": "^8.5.1",
		"helmet": "^6.0.0",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"pg": "^8.8.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.6",
		"sharp": "^0.31.0",
		"typeorm": "^0.3.9"
	},
	"devDependencies": {
		"@jorgebodega/typeorm-seeding": "^5.0.2",
		"@nestjs/cli": "^9.1.2",
		"@nestjs/schematics": "^9.0.3",
		"@nestjs/testing": "^9.0.11",
		"@types/bcrypt": "^5.0.0",
		"@types/compression": "^1.7.2",
		"@types/express": "^4.17.13",
		"@types/jest": "^29.0.0",
		"@types/lodash": "^4.14.184",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.7.15",
		"@types/passport-jwt": "^3.0.6",
		"@types/passport-local": "^1.0.34",
		"@types/sharp": "^0.30.5",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"nodemon": "^2.0.19",
		"prettier": "^2.7.1",
		"supertest": "^6.2.4",
		"ts-jest": "^28.0.8",
		"ts-loader": "^9.3.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.2"
	}
}
