{
	"name": "@mbss/nestjs-core",
	"version": "1.0.2",
	"description": "MBSS-Library core package for nestjs, includes default functions to boost startup with nestjs backend from scratch",
	"title": "MBSS-Nestjs Core package",
	"keywords": [
	  "nestjs",
	  "core",
	  "backend",
	  "functions",
	  "scratch",
	  "startup"
	],
	"author": "Michael Bigalke & Sven Stiels",
	"license": "MIT",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"test": "jest --config jestconfig.json",
		"build": "tsc && tsc-alias",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"lint:fix": "tslint --fix -p tsconfig.json -c tslint.json",
		"prepare": "npm run build",
		"prepublishOnly:old": "npm run test & npm run lint",
		"prepublishOnly": "npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags"
	},
	"files": [
	  "dist",
	  "lib",
	  "es"
	],
	"dependencies": {
		"@nestjs/common": "^8.0.6",
		"@nestjs/core": "^8.0.6",
		"@nestjs/jwt": "^8.0.0",
		"@nestjs/passport": "^8.0.1",
		"@nestjs/platform-express": "^8.0.6",
		"@nestjs/schedule": "^1.0.1",
		"@nestjs/serve-static": "^2.2.2",
		"@nestjs/typeorm": "^8.0.2",
		"bcrypt": "^5.0.1",
		"cache-manager": "^3.4.4",
		"chalk": "^4.1.2",
		"class-transformer": "^0.4.0",
		"class-validator": "^0.13.1",
		"compression": "^1.7.4",
		"dotenv": "^10.0.0",
		"express-rate-limit": "^5.3.0",
		"google-auth-library": "^7.9.1",
		"helmet": "^4.6.0",
		"lodash": "^4.17.21",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"pg": "^8.7.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.3.0",
		"sharp": "^0.29.1",
		"typeorm": "^0.2.37",
		"typeorm-seeding": "^1.6.1"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.1.1",
		"@nestjs/schematics": "^8.0.3",
		"@nestjs/testing": "^8.0.6",
		"@types/bcrypt": "^5.0.0",
		"@types/compression": "^1.7.2",
		"@types/express": "^4.17.13",
		"@types/express-rate-limit": "^5.1.3",
		"@types/jest": "^27.0.1",
		"@types/lodash": "^4.14.172",
		"@types/multer": "^1.4.7",
		"@types/node": "^16.7.13",
		"@types/passport-jwt": "^3.0.6",
		"@types/passport-local": "^1.0.34",
		"@types/sharp": "^0.28.6",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "27.1.0",
		"nodemon": "^2.0.12",
		"prettier": "^2.3.2",
		"supertest": "^6.1.6",
		"ts-jest": "^27.0.5",
		"ts-loader": "^9.2.5",
		"ts-node": "^10.2.1",
		"typescript": "^4.4.2"
	},
	"browserslist": [
	  "> 0.5%",
	  "last 2 versions",
	  "Firefox ESR",
	  "not dead",
	  "IE 11",
	  "not IE 10"
	]
}
